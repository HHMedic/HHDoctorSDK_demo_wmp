  padding: 5px 7px;
}

.extend-btn {
  background-color: #fff;
  font-size: 12pt;
  box-sizing: border-box;
  padding: 2pt 10pt;
  border-radius: 14pt;
  border: solid 0.5pt #e4e4e4;
  color: #333;
  margin-left: 10px;
  display: inline-block;
}

.extend-btn:first-child {
  margin-left: 0px;
}

.msg-row {
  width: 100%;
  display: flex;
  margin-top: 20px;
  box-sizing: border-box;
  padding-right: 45px;
}

.msg-row.timestamp {
  flex-direction: column;
  align-items: center;
  padding-right: 0px;
}

.msg-time {
  background-color: #d2d2d2;
  box-sizing: border-box;
  padding: 2px 5px;
  border-radius: 5px;
  font-size: 12px;
  color: #fff;
}

.msg-row.user {
  flex-direction: row-reverse;
  padding-right: 0px;
  padding-left: 45px;
}

.msg-img {
  width: 35px;
  min-width: 35px;
}

.img-head {
  width: 35px;
  height: 35px;
}

.msg-body {
  margin-left: 10px;
  box-sizing: border-box;
  position: relative;
}

.msg-body.user {
  margin-left: 0px;
  margin-right: 10px;
  box-sizing: border-box;
  position: relative;
}

.msg-name {
  font-size: 12px;
  color: #d2d2d2;
}

.msg-content-main {
  display: flex;
  width: 100%;
  align-items: center;
  box-sizing: border-box;
  padding-top: 8px;
}
.msg-disclaimer{
  margin-top:24rpx;
  font-size:24rpx;
  font-weight:400;
  color:rgba(150,150,150,1);
  line-height:36rpx;

}

.msg-content-main.user {
  padding-top: 0px;
  flex-direction: row-reverse;
}

.msg {
  background-color: #fff;
  box-sizing: border-box;
  padding: 15px 15px 15px 10px;
  border-radius: 5px;
  position: relative;
}

.msg::before {
  content: '';
  position: absolute;
  left: -5px;
  top: 0px;
  width: 0px;
  height: 0px;
  border-top: 6px solid #fff;
  border-left: 10px solid transparent;
}

.msg.user {
  background-color: #c4e7ff;
}

.msg.user::before {
  border-top: 0px solid #fff;
  border-left: 0px solid transparent;
}

.msg.user::after {
  content: '';
  position: absolute;
  right: -5px;
  top: 0px;
  width: 0px;
  height: 0px;
  border-top: 6px solid #c4e7ff;
  border-right: 10px solid transparent;
}

.msg.text {
  font-size: 15px;
  color: #666;
}

.msg.user.text {
  color: #333;
}

.msg.image {
  max-width: 100px;
  border-radius: 5px;
  overflow: hidden;
  padding: 0px;
}

.msg.image::before {
  border-top: 0px solid #fff;
  border-left: 0px solid transparent;
}

.msg.audio {
  width: 100px;
  height: 40px;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.sound-icon {
  border: 2px solid #646464;
  border-left: 2px solid transparent;
  border-bottom: 2px solid transparent;
  border-top: 2px solid transparent;
  border-radius: 50%;
  position: absolute;
}

.sound-icon.user {
  border: 2px solid #646464;
  border-right: 2px solid transparent;
  border-bottom: 2px solid transparent;
  border-top: 2px solid transparent;
}

.sound-icon.arc1 {
  width: 0px;
  height: 0px;
  border-width: 3px;
  left: 8px;
  top: 17px;
}

.sound-icon.arc2 {
  width: 10px;
  height: 10px;
  left: 5px;
  top: 13px;
}

.sound-icon.arc3 {
  width: 20px;
  height: 20px;
  left: 0px;
  top: 8px;
}

.sound-icon.arc1.user {
  left: auto;
  right: 8px;
}

.sound-icon.arc2.user {
  left: auto;
  right: 5px;
}

.sound-icon.arc3.user {
  left: auto;
  right: 0px;
}

.sound-length {
  margin-left: 10px;
  line-height: 40px;
  height: 40px;
  font-size: 12px;
  color: #969696;
}

.sound-length.user {
  margin-left: auto;
  margin-right: 10px;
}

.msg-body.card {
  width: 100%;
}

.msg.card {
  width: 100%;
  box-shadow: 6px 6px 30px 1px rgba(0, 0, 0, 0.1);
}

.msg-card-head {
  font-size: 15px;
  line-height: 15px;
  font-weight: bold;
  color: #333;
  box-sizing: border-box;
  padding-bottom: 10px;
  border-bottom: solid 0.5px #d2d2d2;
}

.msg-card-body {
  box-sizing: border-box;
  padding: 10px 0px;
}

.msg-card-body.summary {
  border-bottom: dashed 0.5px #d2d2d2;
}

.msg-card-body.medicine {
  padding-bottom: 5px;
  display: flex;
  flex-direction: column;
}

.summary-row {
  display: flex;
  margin-bottom: 5px;
}

.summary-row:last-child {
  margin-bottom: 0px;
}

.summary-row.tip {
  margin-left: 10px;
}

.summary-row.tip::before {
  content: '';
  position: relative;
  left: -10px;
  top: 7px;
  width: 7px;
  height: 7px;
  border-radius: 50%;
  background-color: #666;
}

.summary {
  font-size: 15px;
  color: #666;
}

.summary.name {
  width: 79px;
}

.summary.value {
  flex-grow: 1;
}

.summary.medicine {
  font-size: 15px;
  color: #333;
}


.show-more {
  margin-top: 10px;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.show-more-text {
  flex-grow: 1;
}

.show-more-text:before {
  content: '详情';
  color: #999;
  font-size: 12px;
}

.show-more-text.custom::before {
  content: '';
  color: #999;
  font-size: 12px;
}


.show-more-text-right::after {
  content: '详情';
  position: relative;
  color: #999;
  font-size: 12px;
  top: -2px;
}

.show-more-text.custom {
  color: #999;
  font-size: 12px;
}

.show-more-text.custom:before {
  content: '';
}

.show-more-arraw {
  background-image: url("https://wmp.hh-medic.com/wmp/resources/image/arrow.png");
  background-size: cover;
  background-repeat: no-repeat;
  background-position: center;
