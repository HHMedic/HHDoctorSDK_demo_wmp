
.msg-gray {
    display: flex;
    font-size: 26rpx;
    color: #3658A6;
    margin: 32rpx 0 19rpx 0;
    align-items: center;
    padding: 0 26rpx;
    line-height:40rpx;
}
.msg-err {
    font-size: 26rpx;
    color: #ec1313;
    padding: 0 26rpx;
    display: flex;
    align-items: center;
    margin:20rpx 0 42rpx 0;
}

.msg-err:before {
    content: '';
    display: block;
    width: 34rpx;
    height: 34rpx;
    margin-right: 15rpx;
    background: url('https://imgs.hh-medic.com/icon/wmp/eleme/msg-err.png') no-repeat;
    background-size: cover;
}

