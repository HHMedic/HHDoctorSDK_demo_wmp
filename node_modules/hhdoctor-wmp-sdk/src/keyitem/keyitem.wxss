.key-container {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  border-radius: 2px;
  background-color: #fff;
}

.key-container.disabled {
  color: #c2c2c2;
}

.key-container:active {
  background-color: #f2f2f2;
}

.key-container.disabled:active {
  background-color: #fff;
}
