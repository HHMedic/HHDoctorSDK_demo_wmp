page {
  width: 100%;
  background-color: #f2f2f2;
  font-family: PingFangSC-Regular, sans-serif;
  font-size: 15px;
  box-sizing: border-box;
  padding-top: 1px;
}

.error {
  width: 100%;
  height: 24px;
  font-size: 24px;
  line-height: 24px;
  text-align: center;
  margin-top: 100px;
}

.main {
  width: 100%;
  box-sizing: border-box;
  padding-bottom: 55px;
}

.panel {
  width: 100%;
  background-color: #fff;
  margin-top: 12px;
}

.panel:first-child {
  margin-top: auto;
}

.panel-card-list {
  box-sizing: border-box;
  padding: 20px 20px 15px 20px;
}

.card-info {
  width: 100%;
  position: relative;
  color: #fff;
  display: flex;
  margin-top: 25px;
}

.card-info:first-child {
  margin-top: auto;
}

.card-info.prime {
  color: #424559;
}

.card-image {
  width: 100%;
}

.card-image.selected::after {
  content: '';
  position: absolute;
  right: 1px;
  top: 0px;
  width: 52px;
  height: 52px;
  background-image: url('https://imgs.hh-medic.com/icon/wmp/card-selected.png');
  background-repeat: no-repeat;
  background-size: 100% 100%;
  -moz-background-size: 100% 100%;
}

.card-image.prime.selected::after {
  background-image: url('https://imgs.hh-medic.com/icon/wmp/card-prime-selected.png');
}

.card-title {
  position: absolute;
  left: 20px;
  top: 25px;
  font-weight: bolder;
  font-size: 18px;
  line-height: 18px;
}

.card-title::after {
  content: '';
  position: absolute;
  right: -25px;
  top: -2px;
  width: 20px;
  height: 22px;
  background-image: url('https://imgs.hh-medic.com/icon/wmp/card-v.png');
  background-repeat: no-repeat;
  background-size: 100% 100%;
  -moz-background-size: 100% 100%;
}

.card-title.prime::after {
  top: -4px;
  width: 23px;
  height: 25px;
  background-image: url('https://imgs.hh-medic.com/icon/wmp/card-prime-v.png');
}

.card-desc-list {
  position: absolute;
  left: 25px;
  top: 60px;
}

.card-desc {
  font-size: 16px;
  line-height: 16px;
  margin-top: 13px;
  margin-left: 12px;
  position: relative;
}

.card-desc:first-child {
  margin-top: auto;
}

.card-desc::before {
  content: '';
  position: absolute;
  left: -12px;
  top: 5.5px;
  width: 5px;
  height: 5px;
  background-image: url('https://imgs.hh-medic.com/icon/wmp/dot-white.png');
  background-repeat: no-repeat;
  background-size: 100% 100%;
  -moz-background-size: 100% 100%;
}

.card-desc.prime::before {
  background-image: url('https://imgs.hh-medic.com/icon/wmp/dot-black.png');
}

.card-desc.prime.key {
  line-height: 22px;
  height: 22px;
  color: #fff;
  text-align: center;
  background-color: #424559;
  border-radius: 11px;
}

.card-price {
  position: absolute;
  left: 20px;
  bottom: 20px;
  font-size: 12px;
  line-height: 12px;
}

.price-unit {
  font-size: 9px;
}

.price-num {
  font-size: 23px;
  font-weight: bolder;
}

.card-price-raw {
  position: absolute;
  left: 94px;
  bottom: 20px;
  font-size: 11px;
  color: #b4e0ff;
  line-height: 12px;
}

.card-price-raw.prime {
  color: #9f8b60;
}

.card-price-raw::before {
  content: '';
  width: 50px;
  height: 1px;
  border-top: solid 1px #b4e0ff;
  position: absolute;
  left: 0px;
  top: 5px;
  transform: rotateZ(5deg);
}

.card-price-raw.prime::before {
  border-top: solid 1px #9f8b60;
}

.price-num-raw {
  font-size: 15px;
  font-weight: normal;
}

.valid-date {
  color: #646464;
  width: 100%;
  margin-top: 13px;
  font-size: 13px;
}

.row {
  width: 100%;
  box-sizing: border-box;
  padding-left: 16px;
}

.row-cell {
  width: 100%;
  height: 60px;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  border-bottom: solid 0.5px #f2f2f2;
  box-sizing: border-box;
  padding-right: 15px;
}

.row-label {
  color: #333;
  min-width: 65px;
}

.row-value {
  flex-grow: 1;
}

.row-extbtn {
  width: 100px;
  height: 25px;
  border-radius: 13px;
  border: solid 0.5px #0592f5;
  text-align: center;
  line-height: 25px;
  font-size: 14px;
  color: #0592f5;
}

.row-extbtn:active {
  background-color: #0592f5;
  color: #fff;
}

.row-extbtn.disabled {
  border-color: #646464;
  color: #646464;
}

.row-extbtn.disabled:active {
  background-color: #fff;
  color: #646464;
}

.row-license {
  margin-top: 10px;
  margin-bottom: 10px;
  width: 100%;
  box-sizing: border-box;
  padding-left: 13px;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}

.icon {
  width: 19px;
  height: 19px;
}

.icon.ok {
  background-image: url('https://imgs.hh-medic.com/icon/wmp/icon-ok.png');
  background-repeat: no-repeat;
  background-size: 100% 100%;
  -moz-background-size: 100% 100%;
}

.license {
  margin-left: 5px;
  font-size: 13px;
  color: #969696;
}

.license.name {
  margin-left: auto;
  color: #0592f5;
}

.btn {
  position: fixed;
  left: 0px;
  bottom: 0px;
  width: 100%;
  height: 55px;
  color: #fff;
  text-align: center;
  line-height: 55px;
  font-size: 18px;
  font-weight: bold;
  background-color: #0592f5;
  z-index: 9;
  border-radius: 0px;
}

.btn:active {
  background-color: #107bf2;
}

.hidden {
  display: none;
}

.btn-disabled {
  background-color: #969696;
}

.btn-disabled:active {
  background-color: #969696;
}

.view-right {
  position: absolute;
  right: 34px;
  top: 24px;
  width: 75px;
  height: 20px;
  line-height: 20px;
  text-align: center;
  border-radius: 10px;
  font-size: 14px;
  color: #fff;
  border: solid 0.5px #fff;
}

.view-right.prime {
  color: #424559;
  border: solid 0.5px #424559;
}
