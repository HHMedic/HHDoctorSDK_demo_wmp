/* components/innerpages/hh-ehr/ehr-filing-list/ehr-filing-list.wxss */
.page{
  border-top:1px solid #eee;
  min-height:100vh;
  background:#f2f2f2;
}
.filing-box{
  background:#fff;
  padding:20rpx;
  margin-bottom:20rpx;
}
.filing-top{
  display:flex;
  justify-content: space-between;
  align-items: center;
  height:95rpx;
  border-bottom:1px solid #eee;
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(51,51,51,1);
}
.filing-name{
  font-size:30rpx;
  color:rgba(102,102,102,1);
}
.filing-content{
  padding:29rpx 30rpx;
}
.filing-text{
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:300;
  color:rgba(153,153,153,1);
  line-height:42rpx;
  display:-webkit-box;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2;
  overflow:hidden;
}
.filing-scroll{
  height:176rpx;
  white-space: nowrap;
  margin-top:22rpx;
}


.filing-nav{
  width:189rpx;
  height:189rpx;
  background:url(data:image/png;base64,) no-repeat;
  background-size:cover;
  position:fixed;
  bottom:34rpx;
  right:25rpx;
}
.no-data{
  width:500rpx;
  display:block;
  margin:100rpx auto 0 auto;

}


