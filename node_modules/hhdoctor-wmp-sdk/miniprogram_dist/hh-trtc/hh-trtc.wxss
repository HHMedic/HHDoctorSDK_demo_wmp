/* components/hh-trtc/hh-trtc.wxss */
.page-bg {
  width: 100%;
  height: 100vh;
  background: #fff;
  top: 0;
  left: 0;
  position: absolute;
  z-index: 98;
  background-repeat: no-repeat;
  background-size: cover;
  background-position: center;
}

.hh-live {
  top: 0;
  left: 0;
  position: absolute;
  height: 100vh;
  z-index: 101;
}

.room-modal-wait {
  position: fixed;
  bottom: 0;
  left: 0;
  height: 225rpx;
  width: 100%;
  z-index: 105;
  background: rgba(0, 0, 0, .5);
  text-align: center;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start;
}

.wait-text {
  font-size: 36rpx;
  margin-top: 20rpx;
  color: #fff;
}

.wait-icon {
  margin-top: 18rpx;
  width: 120rpx;
  height: 120rpx;
  background: url('data:image/png;base64,') no-repeat;
  background-size: cover;
}

/* 授权部分 */
.auth-box {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100vh;
  background: rgba(0, 0, 0, .5);
  z-index: 120;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.auth-text {
  color: #fff;
  font-size: 29rpx;
  text-align: center;
  line-height: 42rpx;
  width: 642rpx;
  margin: 60rpx auto;
  white-space: pre-wrap;
}

.auth-button {
  width: 660rpx;
  height: 100rpx;
  line-height: 100rpx;
  background: #19be6b;
  border-radius: 10px;
  color: #fff;
  font-size: 32rpx;
  margin-bottom: 25rpx;
}

.hide-tip {
  position: fixed;
  z-index: 10000;
  top: 485rpx;
  left: 120rpx;
  display: flex;
  align-items: center;
  background: rgba(0, 0, 0, .4);
  height: 60rpx;
  font-size: 28rpx;
  color: #dcdcdc;
  padding: 0 10rpx 0 20rpx;
  border-radius: 8rpx;
  line-height: 60rpx;
}

.hide-tip-close {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  margin-left: 10rpx;
  background: url('https://imgs.hh-medic.com/icon/wmp/hide-tip-close.png') no-repeat;
  background-size: cover;
}

.hide-tip-wrap {
  position: fixed;
  z-index: 998;
  left: 0rpx;
  bottom: 35vh;
  width: 100%;
  height: 60rpx;
  text-align: center;
}

.hide-tip-text {
  color: #fff;
  font-size: 26rpx;
  height: 60rpx;
  box-sizing: border-box;
  padding: 18rpx;
  border-radius: 10rpx;
  background-color: rgba(0, 0, 0, .4);
}