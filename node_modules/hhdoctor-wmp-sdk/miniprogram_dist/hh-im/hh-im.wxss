page {
  height: 100%;
  font-family: PingFangSC-Regular, sans-serif;
}

video {
  width: 100%;
}

.hidden {
  display: none;
}

.main-container {
  position: fixed;
  width: 100%;
  height: 100%;
  left: 0px;
  top: 0px;
  box-sizing: border-box;
  background-color: #f0f0f0;
}


.callbtn-panel-container {
  position: fixed;
  left: 0px;
  width: 100%;
  height: 99px;
  background-color: rgba(255, 255, 255, 0.9);
  z-index: 7;
}

.callbtn-panel {
  width: 100%;
  height: 99px;
  display: flex;
  box-sizing: border-box;
  padding-top: 2px;
  padding-left: 26px;
  padding-right: 26px;
  padding-bottom: 27px;
  z-index: 8;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.call-doctor {
  flex-grow: 1;
  height: 55px;
  max-width: 135px;
  width: 100%;
  border-radius: 5px;
  text-align: center;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
  padding: 15px;
  border: solid 1px var(--hh-main-color, #0592f5);
}

.btn-call-img {
  width: 26px;
  height: 26px;
  margin-right: 7px;
}

.btn-call-text {
  font-size: 17px;
  color: var(--hh-main-color, #0592f5);
}

.call-btn-sept {
  min-width: 26px;
}

.msg-panel-scroll {
  /* position: fixed; */
  left: 0px;
  /* top: 109px; */
  top: 120px;
  width: 100%;
  background-color: #f0f0f0;
  box-sizing: border-box;
  padding: 0 10px 0px 10px;
}


.extend-panel {
  position: absolute;
  left: 0px;
  top: -40px;
  height: 40px;
  width: 100%;
  overflow: hidden;
  white-space: nowrap;
  background-color: #fff;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.extend-view {
  height: 100%;
  box-sizing: border-box;
  padding: 5px 7px;
}

.extend-btn {
  background-color: #fff;
  font-size: 12pt;
  box-sizing: border-box;
  padding: 2pt 10pt;
  border-radius: 14pt;
  border: solid 0.5pt #e4e4e4;
  color: #333;
  margin-left: 10px;
  display: inline-block;
}

.extend-btn:first-child {
  margin-left: 0px;
}

.msg-row {
  width: 100%;
  display: flex;
  margin-top: 20px;
  box-sizing: border-box;
  padding-right: 40px;
}

.msg-row.timestamp {
  flex-direction: column;
  align-items: center;
  padding-right: 0px;
}

.msg-time {
  background-color: #d2d2d2;
  box-sizing: border-box;
  padding: 2px 5px;
  border-radius: 5px;
  font-size: 12px;
  color: #fff;
}

.msg-row.user {
  flex-direction: row-reverse;
  padding-right: 0px;
  padding-left: 40px;
}

.msg-img {
  width: 35px;
  min-width: 35px;
}

.img-head {
  width: 35px;
  height: 35px;
}

.msg-body {
  margin-left: 10px;
  box-sizing: border-box;
  position: relative;
}

.msg-body.user {
  margin-left: 0px;
  margin-right: 10px;
  box-sizing: border-box;
  position: relative;
}

.msg-name {
  font-size: 12px;
  color: #969696;
}

.msg-content-main {
  display: flex;
  width: 100%;
  align-items: center;
  box-sizing: border-box;
  padding-top: 8px;
}

.msg-notrans-tips {
  width: 100%;
  font-size: 26rpx;
  line-height: 46rpx;
  color: #969696;
}

.msg-disclaimer {
  margin-top: 24rpx;
  font-size: 26rpx;
  font-weight: 400;
  color: rgba(150, 150, 150, 1);
  line-height: 36rpx;
  /* display: flex; */
  /* justify-content: center; */
}

.msg-disclaimer.warn {
  color: #F62525;
}

.msg-disclaimer.warn.center {
  text-align: center;
}

.msg-disclaimer-link {
  color: var(--hh-main-color, #0592f5);
  text-decoration: underline;
}

.msg-content-main.user {
  padding-top: 0px;
  flex-direction: row-reverse;
}

.msg {
  background-color: #fff;
  box-sizing: border-box;
  padding: 15px 10px 10px 10px;
  border-radius: 5px;
  position: relative;
}

.msg::before {
  content: '';
  position: absolute;
  left: -5px;
  top: 0px;
  width: 0px;
  height: 0px;
  border-top: 6px solid #fff;
  border-left: 10px solid transparent;
}

.msg.user {
  background-color: #c4e7ff;
}

.msg.user::before {
  border-top: 0px solid #fff;
  border-left: 0px solid transparent;
}

.msg.user::after {
  content: '';
  position: absolute;
  right: -5px;
  top: 0px;
  width: 0px;
  height: 0px;
  border-top: 6px solid #c4e7ff;
  border-right: 10px solid transparent;
}

.msg.text {
  font-size: 15px;
  color: #666;
  word-wrap: break-word;
  word-break: break-all;
  white-space: normal
}

.text-link {
  color: var(--hh-main-color, #0592f5);
}

.text-link::after .text-link::before {
  content: ' ';
}


.msg.user.text {
  color: #333;
}

.msg.image {
  /* max-width: 100px; */
  width: 330rpx;
  max-height: 400rpx;
  border-radius: 5px;
  overflow: hidden;
  padding: 0px;
}

.msg.image::before {
  border-top: 0px solid #fff;
  border-left: 0px solid transparent;
}

.msg.audio {
  width: 100px;
  height: 40px;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.sound-icon {
  border: 2px solid #646464;
  border-left: 2px solid transparent;
  border-bottom: 2px solid transparent;
  border-top: 2px solid transparent;
  border-radius: 50%;
  position: absolute;
}

.sound-icon.user {
  border: 2px solid #646464;
  border-right: 2px solid transparent;
  border-bottom: 2px solid transparent;
  border-top: 2px solid transparent;
}

.sound-icon.arc1 {
  width: 0px;
  height: 0px;
  border-width: 3px;
  left: 8px;
  top: 17px;
}

.sound-icon.arc2 {
  width: 10px;
  height: 10px;
  left: 5px;
  top: 13px;
}

.sound-icon.arc3 {
  width: 20px;
  height: 20px;
  left: 0px;
  top: 8px;
}

.sound-icon.arc1.user {
  left: auto;
  right: 8px;
}

.sound-icon.arc2.user {
  left: auto;
  right: 5px;
}

.sound-icon.arc3.user {
  left: auto;
  right: 0px;
}

.sound-length {
  margin-left: 10px;
  line-height: 40px;
  height: 40px;
  font-size: 12px;
  color: #969696;
}

.sound-length.user {
  margin-left: auto;
  margin-right: 10px;
}

.msg-body.card {
  width: 100%;
}

.msg.card {
  width: 100%;
  box-shadow: 6px 6px 30px 1px rgba(0, 0, 0, 0.1);
}

.msg-card-head {
  font-size: 15px;
  line-height: 15px;
  font-weight: bold;
  color: #333;
  box-sizing: border-box;
  padding-bottom: 10px;
  border-bottom: solid 0.5px #d2d2d2;
}

.msg-card-body {
  box-sizing: border-box;
  padding: 10px 0px;
}

.msg-card-body.image,
.msg-card-body.expert {
  padding: 0px;
  position: relative;
}

.msg-card-body image {
  display: block;
}

.msg-card-body.summary {
  border-bottom: dashed 0.5px #d2d2d2;
}

.msg-card-body.medicine {
  padding-bottom: 5px;
  display: flex;
  flex-direction: column;
}

.msg-card-mask {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  background: rgba(0, 0, 0, .2);
  min-height: 60rpx;
  font-size: 26rpx;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  padding: 5rpx 10rpx;
  color: #fff;
}

.summary-row {
  display: flex;
  margin-bottom: 5px;
}

.summary-row:last-child {
  margin-bottom: 0px;
}

.summary-row.tip {
  margin-left: 10px;
}

.summary-row.tip::before {
  content: '';
  position: relative;
  left: -10px;
  top: 7px;
  width: 7px;
  height: 7px;
  border-radius: 50%;
  background-color: #666;
}

.summary {
  font-size: 15px;
  color: #666;
}

.summary-tip {
  font-size: 26rpx;
  color: red;
}


.summary.name {
  width: 79px;
}

.summary.value {
  flex-grow: 1;
  width: 160rpx;
}

.summary.medicine {
  font-size: 15px;
  color: #333;
}


.show-more {
  margin-top: 10px;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.show-more-default {
  flex-grow: 1;
  color: #999;
  font-size: 12px;
}

.show-more-text {
  flex-grow: 1;
  color: #999;
  font-size: 12px;
}

.show-more-text:before {
  content: '详情';
}

.show-more-text.custom::before {
  content: '';
  color: #999;
  font-size: 12px;
}

.show-more-right {
  color: #999;
  font-size: 12px;
  width: 98%;
  display: flex;
  justify-content: space-between;
}

.show-more-right:after {
  content: '更多权益';
}


.show-more-text-right::after {
  content: '详情';
  position: relative;
  color: #999;
  font-size: 12px;
  top: -2px;
}

.show-more-text.custom {
  color: #999;
  font-size: 12px;
}

.show-more-text.custom:before {
  content: '';
}

.show-more-arraw {
  background-image: url("https://wmp.hh-medic.com/wmp/resources/image/arrow.png");
  background-size: cover;
  background-repeat: no-repeat;
  background-position: center;
  width: 6px;
  height: 9px;
  margin-left: 5rpx;
}

.show-more-btn {
  width: 100%;
  height: 35px;
  border-radius: 5px;
  background-color: var(--hh-main-color, #0592f5);
  color: #fff;
  text-align: center;
  line-height: 35px;
  font-size: 14px;
}

.show-more-btn.btn-b2c {
  background-color: #FC8E20;
}

.show-more-btn.view-order {
  background-color: #fff;
  color: var(--hh-main-color, #0592f5);
  border: 1px solid var(--hh-main-color, #0592f5);
}

.disabled.disabled {
  background-color: #d2d2d2;
  color: #000;
}

.medicine-row {
  box-sizing: border-box;
  padding: 5px 0px;
  border-bottom: dashed 0.5px #d2d2d2;
  width: 100%;
}

.medicine {
  width: 100%;
  display: flex;
  flex-direction: row;
  font-size: 15px;
  font-weight: bold;
  line-height: 23px;
  color: #323232;
  align-items: center;
}

.medicine-name {
  flex-grow: 1;
}


.medicine-rx {
  width: 76rpx;
  height: 30rpx;
  background-image: url('https://imgs.hh-medic.com/icon/wmp/icon-rx.png');
  background-position: center;
  background-size: cover;
  margin-right: 10rpx;
}

.medicine-count {
  min-width: 55px;
  text-align: right;
}

.medicine-count::before {
  content: 'x';
  position: relative;
  left: -8px;
}

.medicine-remark {
  margin: 10rpx 0;
  font-size: 28rpx;
  line-height: 42rpx;
  color: #646464;
  font-weight: 500;
}

.equity-body {
  background: url('https://imgs.hh-medic.com/icon/wmp/equity-body.png') no-repeat;
  background-size: cover;
  width: 100%;
  height: 432rpx;
  padding: 34rpx 0 50rpx 0;
  box-sizing: border-box;
  border-radius: 10rpx;
}

.equity-title {
  color: #424559;
  font-size: 30rpx;
  font-weight: 500;
  display: flex;
  justify-content: center;
  align-items: center;
}

.equity-title::before,
.equity-title::after {
  content: '';
  display: block;
  width: 10rpx;
  height: 10rpx;
  background: #424559;
  border-radius: 50%;
  margin: 0 15rpx;
}

.equity-list {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;

}

.equity-item {
  width: 138rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 22rpx;
  margin: 40rpx 0 0 0;
  font-family: PingFang SC Regular, PingFang SC Regular-Regular;
}

.equity-item text {
  display: block;
}

.equity-icon {
  width: 70rpx;
  height: 70rpx;
  display: block;
  margin-bottom: 11rpx;
}

.expert-wrap {
  margin-top: 20rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
}

.expert-image {
  width: 96rpx;
  height: 124rpx;
}

.expert-info-wrap {
  flex-wrap: 1;
  margin-left: 22rpx;
  font-size: 28rpx;
  line-height: 36rpx;
  color: #666;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.expert-name {
  color: #333;
}

.expert-time-title {
  margin-top: 20rpx;
}

.expert-time {
  color: #333;
}

.font-bold {
  font-weight: bold;
}

.msg-drug-image {
  width: 515rpx;
  display: block;
}

.real-name-wrap {
  position: fixed;
  left: 0;
  top: 300rpx;
  width: 100%;
  height: 80rpx;
  background-color: #FFF8ED;
  z-index: 5;
  font-size: 28rpx;
  color: #E65A16;
  line-height: 80rpx;
  text-align: center;
}

.real-name-btn {
  color: var(--hh-main-color, #0592f5);
  margin-left: 14rpx;
  text-decoration: underline;
}

.auth-box {
  position: fixed;
  top: 0;
  width: 100vw;
  height: 100vh;
  left: 0;
  background: rgba(0, 0, 0, .5);
  display: flex;
  align-items: center;
  justify-content: center;

}

.auth-pannel {
  background: #fff;
  width: 590rpx;
  border-radius: 15rpx;
  text-align: center;
}

.auth-title {
  margin-top: 74rpx;
  font-size: 34rpx;
  color: #323232;
}

.auth-content {
  margin: 41rpx 0 61rpx 0;
  font-size: 26rpx;
  color: #8d8d8d;
  padding: 0 38rpx;
  text-align: justify;
  line-height: 47rpx;
}

.auth-btns {
  display: flex;
  justify-content: space-between;
  border-top: 1px solid #e6e6e6;

}

.auth-reject,
.auth-confirm {
  width: 50%;
  text-align: center;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 30rpx;
}

.auth-reject {
  color: #646464;
  border-right: 1px solid #e6e6e6;
}

.auth-confirm {
  color: #0093fd;
}

.safe-bottom {
  padding-bottom: env(safe-area-inset-bottom)
}

.msg-card-mark {
  width: 100rpx;
  height: 30rpx;
  background: #2786F2;
  border-radius: 8rpx;
  color: #fff;
  padding: 3rpx 10rpx;
  font-size: 22rpx;
  vertical-align: middle;
  font-family: PingFangSC-Regular;
  font-weight: 400;
}