/* 授权部分 */
.auth-box {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100vh;
  background: rgba(0, 0, 0, .5);
  z-index: 1200;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.auth-text {
  color: #fff;
  font-size: 29rpx;
  text-align: center;
  line-height: 42rpx;
  width: 642rpx;
  margin: 60rpx auto;
  white-space: pre-wrap;
}

.auth-button {
  width: 660rpx;
  height: 100rpx;
  line-height: 100rpx;
  background: #19be6b;
  border-radius: 10px;
  color: #fff;
  font-size: 32rpx;
}

