.page {
    width: 100vw;
    min-height: 100vh;
    background: #f2f2f2;
    font-family: PingFangSC-Regular;
    padding-bottom:160rpx;
    overflow:hidden;
}

.msg-tip {
    min-height: 70rpx;
    line-height:40rpx;
    background: #FFF8ED;
    padding: 8rpx 30rpx;
    display: flex;
    align-items: center;
    color: #E65A16;
    font-size: 26rpx;
}

.msg-list {
    height: 100rpx;
    background: #fff;
    padding: 0 30rpx;
    display: flex;
    align-items: center;
    justify-content: flex-start;
    font-size: 32rpx;
    color: #323232;
}

.msg-nickname,
.msg-idcard {
    margin-bottom: 20rpx;
}

.msg-name {
    border-bottom: 1px solid #f2f2f2;
}

.msg-list:before {
    width: 203rpx;
}

.msg-nickname::before {
    content: '成员昵称'
}

.msg-name::before {
    content: '患者姓名'
}

.msg-idcard::before {
    content: '患者身份证'
}

.msg-phone::before {
    content: '联系人电话'
}



.msg-btn-block{
    position:fixed;
    width:100%;
    bottom:0;
    height:140rpx;
    background:#fff;
}
.msg-btn {
    color: #fff;
    font-size: 32rpx;
    background: #0592F5;
    width: 710rpx;
    height:88rpx;
    margin:26rpx auto;
}

.msg-btn::after {
    border: none;
}



.msg-name-input,
.msg-idcard-input,
.msg-phone-input {
    width: 450rpx;
}

.msg-placeholder {
    font-size: 30rpx;
    color: #969696;
}

.msg-close {
    width: 60rpx;
    height: 60rpx;
    background: url('https://imgs.hh-medic.com/icon/wmp/eleme/msg-close.png') no-repeat;
    background-size: cover;
}

.page-recipe,
.page-fail {
    width: 100vw;
    height: 100vh;
    background: #fff;
}

.recipe-icon,
.fail-icon {
    font-size: 34rpx;
    color: #333;
    text-align: center;

}

.recipe-icon::before {
    content: '';
    display: block;
    width: 314rpx;
    height: 201rpx;
    background: url('https://imgs.hh-medic.com/icon/wmp/eleme/recipe-icon.png') no-repeat;
    background-size: cover;
    margin: 175rpx auto 43rpx auto;
}

.load-circle {
    margin-top: 40rpx;
    display: flex;
    justify-content: center;
}

.circle {
    width: 16rpx;
    height: 16rpx;
    border-radius: 50rpx;
    background: #0592F5;
    margin: 10rpx;
    animation: load .9s linear infinite;
}

.circle:nth-of-type(1) {
    animation-delay: 0.15s;
}

.circle:nth-of-type(2) {
    animation-delay: 0.3s;
}

.circle:nth-of-type(3) {
    animation-delay: 0.45s;
}


@keyframes load {
    0% {
        opacity: 1;
    }

    100% {
        opacity: .1;
    }
}

.fail-icon::before {
    content: '';
    display: block;
    width: 282rpx;
    height: 196rpx;
    background: url('https://imgs.hh-medic.com/icon/wmp/eleme/fail-icon.png') no-repeat;
    background-size: cover;
    margin: 177rpx auto 32rpx auto;
}

.page-fail {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.back-index {
    background: #fff;
    font-size: 34rpx;
    color: #0093fd;
    width: 420rpx;
    height: 88rpx;
    margin-bottom: 20%;
}

.back-index::after {
    border: 1px solid #0093fd;
}

.auth-box {
    position: fixed;
    top: 0;
    width: 100vw;
    height: 100vh;
    left: 0;
    background: rgba(0, 0, 0, .5);
    display: flex;
    align-items: center;
    justify-content: center;

}

.auth-pannel {
    background: #fff;
    width: 590rpx;
    border-radius: 15rpx;
    text-align: center;
}

.auth-title {
    margin-top: 74rpx;
    font-size: 34rpx;
    color: #323232;
}

.auth-content {
    margin: 41rpx 0 61rpx 0;
    font-size: 26rpx;
    color: #8d8d8d;
    padding: 0 38rpx;
    text-align: justify;
    line-height: 47rpx;
}

.auth-btns {
    display: flex;
    justify-content: space-between;
    border-top: 1px solid #e6e6e6;

}

.auth-reject,
.auth-confirm {
    width: 50%;
    text-align: center;
    height: 100rpx;
    line-height: 100rpx;
    font-size: 30rpx;
}

.auth-reject {
    color: #646464;
    border-right: 1px solid #e6e6e6;
}

.auth-confirm {
    color: #0093fd;
}

.page-purchase {
    height: 100vh;
    border: 1px solid #eee;
}

.purchase-icon {
    color: #333;
    font-size: 36rpx;
    font-weight: 500;
    display: flex;
    align-items: center;
    flex-direction: column;
    font-family: PingFang SC Medium, PingFang SC Medium-Medium;
}

.purchase-icon::before {
    content: '';
    display: block;
    width: 350rpx;
    height: 257rpx;
    background: url('https://imgs.hh-medic.com/icon/wmp/eleme/purchase-icon.png') no-repeat;
    background-size: cover;
    margin: 125rpx 0 61rpx 0;
}

.purchase-or,
.purchase-text {
    text-align: center;
    font-size: 36rpx;
    color: #333;
    font-weight: 500;
    font-family: PingFang SC Medium, PingFang SC Medium-Medium;
}

.purchase-text {
    margin-top: 31rpx;
}

.purchase-or {
    margin-top: 117rpx;
}

.purchase-btn {
    width: 650rpx;
    height: 88rpx;
    line-height: 88rpx;
    background: #0093fd;
    font-weight: 400;
    border-radius: 10rpx;
    color: #fff;
    font-size: 34rpx;
    margin: 52rpx auto 0 auto;
    text-align: center;
}

.purchase-statement {
    display: flex;
    align-items: center;
    justify-content: center;
    margin-top: 40rpx;
    font-size: 28rpx;
    color: #646464;
    font-weight: 400;
    font-family: PingFangSC-Regular;
}

.purchase-statement::before {
    content: '';
    display: block;
    width: 32rpx;
    height: 32rpx;
    margin-right: 14rpx;
    background: url('https://imgs.hh-medic.com/icon/wmp/eleme/msg-err.png') no-repeat;
    background-size: cover;
}
.input-close{
    width:38rpx;
    height:38rpx;
    background:url(data:image/png;base64,) no-repeat;
    background-size:cover;
    position: absolute;
    right:32rpx; 
}
.group{
    margin:20rpx auto;
}
.realname-title{
    background:#fff;
    color:#646464;
    height:80rpx;
    padding-left:30rpx;
    font-size:28rpx;
    line-height:80rpx;
    border-bottom:1px solid #E6E6E6;
}
.illness-list{
    padding:15rpx 0;
    background:#fff;
}
.illness-item{
    font-size:30rpx;
    color:#323232;
    padding:0 30rpx;
    min-height:68rpx;
    display:flex;
    align-items: center;
}
.realname-photo{
    background:#fff;
    padding:30rpx;
}
.photo-icon{
    display:flex;
    align-items: center;
    color:#969696;
    font-size: 28rpx;
}

.photo-icon-url{
    width:66rpx;
    height:66rpx;
    margin-right:18rpx;
}
.photo-choose-url{
    width:100%;
    height:100%;
    display:block;
}
.photo-choose{
    position:relative;
    width:120rpx;
    height:120rpx;
    border:0.5px solid #f2f2f2;
}
.photo-close{
    width:26rpx;
    height:26rpx;
    position:absolute;
    right:-13rpx;
    top:-13rpx;
    background-color:#fff;
    background-image:url(https://imgs.hh-medic.com/icon/wmp/realname/del-close.png);
    background-repeat: no-repeat;
    background-size:cover;
    border-radius:50%;
}
.choose-num{
    position:absolute;
    bottom:0;
    left:0;
    right:0;
    height:40rpx;
    line-height: 40rpx;
    color:#fff;
    text-align: center;
    font-size:22rpx;
    background:rgba(0,0,0,.3)
}
.photo-is-upload{
    font-size:26rpx;
    color:#969696;
    margin-top:38rpx;
    display:flex;
    align-items: center;
}
.upload-checked,.upload-check{
    width:40rpx;
    height:40rpx;
    margin-right:15rpx;
}
.upload-check{
    background:url(https://imgs.hh-medic.com/icon/wmp/realname/no-upload-photo.png) no-repeat;
    background-size:cover;
}
.upload-checked{
    background:url(https://imgs.hh-medic.com/icon/wmp/realname/is-upload-photo.png) no-repeat;
    background-size:cover;
}
.illness-check,.illness-checked{
    width:40rpx;
    height:40rpx;
    box-sizing: border-box;
}
.illness-text{
    width:600rpx;
    margin-left:15rpx;
}
.illness-checked{
    background:url(https://imgs.hh-medic.com/icon/wmp/realname/checked-diagnosis.png) no-repeat;
    background-size:cover;
}
.illness-check{
    background:url(https://imgs.hh-medic.com/icon/wmp/realname/check.png) no-repeat;
    background-size:cover;
}
