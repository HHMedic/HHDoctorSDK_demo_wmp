page {
  background-color: #F6F6F6;
}

.tip-wrap {
  width: 100%;
  height: 80rpx;
  background-color: #FFF8ED;
  text-align: left;
  color: #E65A16;
  box-sizing: border-box;
  padding-left: 30rpx;
  line-height: 80rpx;
  font-size: 28rpx;
}

.btn-wrap {
  margin-top: 170rpx;
  width: 100%;
  box-sizing: border-box;
  padding: 0 30rpx;
}

.btn {
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
  font-size: 32rpx;
  color: var(--hh-main-color, #0592f5);
  border: 1px solid var(--hh-main-color, #0592f5);
  border-radius: 44rpx;
  overflow: hidden;
  margin-top: 40rpx;
  box-sizing: border-box;
}

.btn:first-child {
  margin-top: 0;
}

.btn.primary {
  color: #FFFFFF;
  background-color: var(--hh-main-color, #0592f5);
  border: none;
}
.btn.hairline{
  border:0.5px solid #1890FF;
  background:#fff;
  color:#1890FF;
}