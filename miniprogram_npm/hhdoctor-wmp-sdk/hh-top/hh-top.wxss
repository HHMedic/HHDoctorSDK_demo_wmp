/* components/hh-top/hh-top.wxss */
.btn-panel {
    position: fixed;
    left: 0px;
    top: 0px;
    width: 100%;
    height: 128px;
    display: flex;
    flex-direction: column;
    justify-content: center;
    box-sizing: border-box;
    z-index: 9;
  }
  .topbtn-panel {
    width: 100%;
    height: 64px;
    /* background-color: rgba(255, 255, 255, 1); */
    background-color:rgba(0,0,0,0);
    z-index: 9;
    box-sizing: border-box;
    border-bottom: solid 0.5px #ebebeb;
    position:relative;
  }
  .top-btns{
    box-sizing: border-box;
    width:120px;
    position:absolute;
    z-index:10;
    padding-left:9px;
    
  }
  .top-btns-inner{
    display:flex;
    align-items: center;
    justify-content: flex-start;
  }
  .topbtn-personal,.topbtn-naviback{
    width:30px;
    height:30px;
    margin-right:16px;
  }
  .topbtn-personal{
    background:url(data:image/png;base64,) no-repeat;
    background-size:cover;
  }
  .topbtn-naviback{
    background:url(data:image/png;base64,) no-repeat;
    background-size:cover;
  }
  .topbtn-personal.light {
    background: url(data:image/png;base64,) no-repeat;
    background-size:cover;
  }
  .topbtn-naviback.light{
    background:url(data:image/png;base64,) no-repeat;
    background-size:cover;
  }
  .topbtn-title {
    position: absolute;
    top: 30px;
    width: 370rpx;
    right:190rpx;
    height: 24px;
    line-height: 24px;
    text-align: center;
    font-size: 20px;
    font-weight: bold;
    overflow:hidden;
    white-space: nowrap;
    text-overflow:ellipsis;
  }
  
  .topbtn-navitoapp {
    background-color: transparent;
    border: none;
    width: 100%;
    height: 100%;
  }
  
  .topbtn-navitoapp::after {
    content: '';
    border: none;
  }
  
  .disconnect-info {
    position: fixed;
    left: 0px;
    width: 100%;
    text-align: center;
    font-size: 15px;
    color: #f00;
    z-index: 19;
    background-color: rgba(255, 255, 255, 1);
  }
  
  .mainbtn-panel {
    /* position: absolute; */
    left: 0px;
    top: 0px;
    width: 100%;
    height: 64px;
    box-sizing: border-box;
    padding: 10px;
    background-color: rgba(255, 255, 255, 1);
    z-index: 9;
  }
  
  .btn-main {
    background-color: #0592f5;
    height: 100%;
    width: 710rpx;
    margin:auto;
    border-radius: 5px;
    color: #fff;
    text-align: center;
    line-height: 49px;
    font-size: 16px;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
  }
  
  .btn-main::before {
    content: '';
    width: 28px;
    height: 20px;
    margin-right: 10px;
    background-image: url("https://imgs.hh-medic.com/icon/wmp/online-video.png");
    background-size: cover;
    background-repeat: no-repeat;
    background-position: center;
  }



  .demo-status {
    font-size: 13px;
    box-sizing: border-box;
    padding-left: 65px;
    color: #0592f5;
  }
  
  .demo-status-1 {
    display: none;
  }
  
  .demo-status0::before {
    content: '';
    position: absolute;
    left: 40px;
    top: -1px;
    width: 20px;
    height: 20px;
    background-image: url('https://imgs.hh-medic.com/icon/wmp/icon_uncheck.png');
    background-size: cover;
    background-repeat: no-repeat;
    background-position: center;
  }
  
  .demo-status1::before {
    content: '';
    position: absolute;
    left: 40px;
    top: -1px;
    width: 20px;
    height: 20px;
    background-image: url('https://imgs.hh-medic.com/icon/wmp/icon_checked.png');
    background-size: cover;
    background-repeat: no-repeat;
    background-position: center;
  }
  
  .live-panel {
    position: fixed;
    left: 0px;
    width: 100%;
    height: 130px;
    box-sizing: border-box;
    padding: 5px 5px 15px 5px;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    background-color: #fff;
  }
  
  .live-item {
    width: 100%;
    height: 100%;
    box-sizing: border-box;
    padding: 0px 5px;
    position: relative;
  }
  
  .live-item-image {
    width: 100%;
    height: 100%;
  }
  
  .live-item-image.status::after {
    content: '';
    position: absolute;
    left: 19px;
    top: 15px;
    width: 66.5px;
    height: 17px;
    background-size: cover;
    background-repeat: no-repeat;
    background-position: center;
  }
  
  .live-item-image.status.active::after {
    background-image: url('https://imgs.hh-medic.com/icon/wmp/live-active.png');
  }
  
  .live-item-image.status.inactive::after {
    background-image: url('https://imgs.hh-medic.com/icon/wmp/live-inactive.png');
  }
  
  /* 顶部成员列表 */
  .member-wrap {
    position: fixed;
    overflow: hidden;
    left: 0;
    font-family: PingFang SC;
    text-align: center;
    padding: 0 20rpx;
    width: 750rpx;
    z-index: 8;
    box-sizing: border-box;
    background: #fff;
    border-top:1px solid #E6E6E6;
  }

  .member-bg{
    position:fixed;
    top:0;
    bottom:0;
    left:0;
    right:0;
    background:rgba(0,0,0,.3);  
    z-index: 8;
  }
  

  .member-list::before{
    display:block;
    font-size: 32rpx;
    color:#323232;
    height:110rpx;
    font-weight: 400;
    font-family: PingFangSC-Regular;
    line-height:100rpx;
  }
