<view class='page'>
 <view class='equity-title'>{{_request.equityTitle}}</view>
 <view class='equity-box'>
    <view class='equity-item {{item.checkExpire?"filter":""}}' wx:for="{{productRightsList}}" wx:key='index' bindtap='_viewMoreRightList' data-index="{{index}}">
       <image src='{{item.icon}}' class='equity-icon'></image>
       <text class='equity-text'>{{item.title}}</text>
    </view>
    <view class='equity-item' bindtap='_viewMoreRightList'>
       <image src='{{moreIcon}}' class='equity-icon'></image>
       <text>更多权益</text>
    </view>
 </view>
</view>