/* components/realname-input/realname-input.wxss */
.bind-field{
    background:#fff;
    height:100rpx; 
    display:flex;
    align-items: center;   
    color:#323232;
    font-size:30rpx;
    padding:0 32rpx;
    position: relative;
    border-bottom:1px solid #F4F4F4;

}
.bind-field input{
    width:380rpx;
}
.bind-field picker{
  width:410rpx;
}
.field-label{
    width:180rpx;
    margin-right:20rpx;
}
.input-phone{
    width:380rpx;
}
.field-readonly{
    font-size: 34rpx;
    font-weight: bold;
    color:#323232;
}
.input-close{
    width:80rpx;
    height:80rpx;
    background:url(data:image/png;base64,) no-repeat;
    background-size:38rpx;
    background-position:center;
    position: absolute;
    right:10rpx;    
}
.phone-tip{
    color:#EC1313;
    font-size:26rpx;
    line-height:40rpx;
    display:flex;
    padding:36rpx 32rpx;
    align-items: stretch;

}
.phone-tip::before,.passport-tip::before{
    content:'';
    display:block;
    width:40rpx;
    height:40rpx;
    background:url(data:image/png;base64,) no-repeat;
    background-size:cover;
    background-position: center;
    background-size:34rpx 34rpx;
    margin-right:10rpx;
}
.arrow::after{
    content:'';
    display: block;
    width:18rpx;
    height:30rpx;
    background:url(data:image/png;base64,) no-repeat;
    background-size:cover;
    position: absolute;
    right:32rpx;
    top:32rpx;
    pointer-events: none;
  }
  